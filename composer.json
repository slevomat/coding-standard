{
	"name": "slevomat/coding-standard",
	"license": "MIT",
	"description": "Slevomat Coding Standard for PHP_CodeSniffer extends Consistence Coding Standard by providing sniffs with additional checks.",
	"config": {
		"preferred-install": "source"
	},
	"require": {
		"php": "^7.0",
		"jakub-onderka/php-parallel-lint": "^0.9.2",
		"squizlabs/php_codesniffer": "^2.6.0"
	},
	"require-dev": {
		"consistence/coding-standard": "^0.11.0",
		"phing/phing": "^2.9",
		"phpstan/phpstan": "dev-dev#1cb3904e17",
		"phpunit/phpunit": "^5.0",
		"satooshi/php-coveralls": "^1.0"
	},
	"autoload-dev": {
		"psr-4": {
			"SlevomatCodingStandard\\": ["SlevomatCodingStandard/", "tests/"]
		},
		"files": [
			"vendor/consistence/coding-standard/tests/Sniffs/TestCase.php"
		]
	}
}
