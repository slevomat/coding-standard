{
	"name": "slevomat/coding-standard",
	"description": "Slevomat Coding Standard for PHP_CodeSniffer extends Consistence Coding Standard by providing sniffs with additional checks.",
	"license": "MIT",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"bin-dir": "bin"
	},
	"require": {
		"php": "^7.0",
		"squizlabs/php_codesniffer": "^2.8"
	},
	"require-dev": {
		"consistence/coding-standard": "^0.13",
		"jakub-onderka/php-parallel-lint": "^0.9.2",
		"phing/phing": "^2.16",
		"phpstan/phpstan": "^0.6.3",
		"phpunit/phpunit": "^6.0.1",
		"satooshi/php-coveralls": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"SlevomatCodingStandard\\": "SlevomatCodingStandard"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SlevomatCodingStandard\\": "tests"
		}
	}
}
